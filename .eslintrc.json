{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "error",
    "quotes": "warn",
    "no-unused-vars": "error",
    "max-len": "error",
    "object-curly-newline": "off",
    "import/extensions": [0, { "<js>": "always" }],
    "no-underscore-dangle": "off",
    "function-paren-newline": "off",
    "implicit-arrow-linebreak": "off"
  }
}
