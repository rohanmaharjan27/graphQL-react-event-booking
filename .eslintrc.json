{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "error",
    "quotes": "warn",
    "no-unused-vars": "error",
    "max-len": "error",
    "object-curly-newline": "off"
  }
}
